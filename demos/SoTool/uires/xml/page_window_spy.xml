<?xml version="1.0" encoding="utf-8"?>
<window size="-2,-2" layout="vbox" interval="5">
  <captureButton name="img_capture_host" skin="skin_spyhost" tip="定位待SPY的SOUI窗口"/>
  <tabctrlex size="-2,0" weight="1" colorTextPush="#FFFF00" tabInterSize="0" tabWidth="100" tabHeight="30" colorText="#FF00FF" drawFocusRect="0" show="1" curSel="0">
    <socrollviewStyle  sbWid="10" sbArrowSize="0"/>
    <pages>
      <pageex title="窗口信息" tip="窗口信息">
        <text pos="10,10">窗口信息:</text>
        <window pos="{30,[5" size="-1,-1" layout="hbox">
        <text>窗口句柄：</text>
          <text name="wnd_hwnd"/>
        </window>
        <window pos="{,[5" size="-1,-1" layout="hbox">
          <text>窗口类名：</text>
          <text name="wnd_class"/>
        </window>
        <window pos="{,[5" size="-1,-1" layout="hbox">
          <text>窗口标题：</text>
          <text name="wnd_title"/>
        </window>
        <window pos="{,[5" size="-1,-1" layout="hbox">
          <text>窗口矩形：</text>
          <text name="wnd_rect"/>
        </window>
        <window pos="{,[5" size="-1,-1" layout="hbox">
          <text>进程ID：</text>
          <text name="wnd_proid"/>
        </window>
        <window pos="{,[5" size="-1,-1" layout="hbox">
          <text>程序路径：</text>
          <text name="wnd_exepath"/>
        </window>
      </pageex>
      <pageex title="窗口样式:" tip="窗口样式">
        <text pos="10,10">窗口样式</text>
        <window pos="{30,[5" name="wnd_style" layout="gridLayout" columnCount="2" xInterval="15" yInterval="5" xGravity="left">          
        </window>
      </pageex>
      <pageex title="窗口扩展样式:" tip="窗口扩展样式">
        <text pos="10,10">窗口扩展样式</text>
        <window pos="{30,[5" name="wnd_exstyle" layout="gridLayout" columnCount="2" xInterval="15" yInterval="5" xGravity="left">
        </window>
      </pageex>
    </pages>
  </tabctrlex>
</window>