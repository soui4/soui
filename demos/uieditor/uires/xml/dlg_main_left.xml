<?xml version="1.0"?>
<tabctrl size="-2,-2" tabAlign="middle" tabHeight="30" tabSkin="skin_tab_skin" animateSteps="10" text-x="10">
      <page title="@string/project_struct" >
        <treeview size="-2,-2" name="workspace_treeview" multiSel="1"  hasLines="1" indent="20">
          <template height="30" layout="hbox" skin="skin_item_bk" trackMouseEvent="1" gravity="center" interval="5"  font="size:16">
            <img name="img_state" size="16,16" skin="skin_filetypes" iconIndex="0"/>
            <stack size="-1,-1" curSel="0" aniType="none" samePageSize="1">
              <text size="-1,-1" name="txt_label" colorText="#FFFFFF"/>
              <edit size="-2,-2" name="edt_label" margin="2"/>
            </stack>
          </template>
        </treeview>
      </page>
      <page title="@string/xml_struct">
        <treectrl size="-2,-2" name="uidesigner_wnd_xmltree" hasLines="1" toggleSkin="skin_tree_toggle_png" colorBkgnd="#FFFFFF12" itemHeight="30" colorItemText="#FFFFFF" colorItemSelText="#FFFFFF" colorItemSelBkgnd="@color/itemSelected" />
      </page>
</tabctrl>