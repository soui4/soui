<?xml version="1.0" encoding="utf-8"?>

<root>
  <user_widgets>
    <!-- 用户没有注册的自定义控件，自动识别为它的父类型,test_widget为用户自定义的类型,parent为父类型,编辑时自动创建为父类型 -->
    <test_widget parent="SWindow"/>
  </user_widgets>

  <toolbar>
    <icons class_name="imglist" name="widget_icons" src="file:img/ctrl_icons.png" states="37"/>
  </toolbar>
  <controls>
    <!-- pos和size都不是必须的，设计器根据鼠标按下的位置设置pos -->
    <!-- 控件的大小最好是8的倍数，方便对齐 -->

    <SOUI  icon="0">
      <SOUI name="window" title="布局" bigIcon="ICON_LOGO:32" smallIcon="ICON_LOGO:16" width="400" height="300" margin="1,5,5,5" wndType="8" toolWindow="1"></SOUI>
        <groups>
        <propgroup name="属性: SOUI">
          <proptext name ="trCtx" extendType="string" description="在语言翻译时作为context使用"/>
          <proptext name ="title" extendType="string" description="宿主窗口标题"/>
          <proptext name ="width" extendType="dim" autoComplete="1" description="默认宿主宽度"/>
          <proptext name ="height" extendType="dim" autoComplete="1" description="默认宿主高度"/>

          <proptext name ="maxInset" extendType="dim" autoComplete="1" />

          <proprect name ="margin" extendType="dim" autoComplete="1" description="窗口拉伸调节区尺寸"/>
          <propsize name ="minsize" extendType="dim" autoComplete="1" description="最小尺寸" />
          <proptext name ="wndStyle" extendType="string" description="宿主样式,16进制"/>
          <proptext name ="wndStyleEx" extendType="string" description="拓展样式,16进制"/>
          <propoption name ="resizable" extendType="undef" options="是:1|否:0" description="大小可变标志"/>
          <propoption name ="translucent" extendType="undef" options="是:1|否:0" description="宿主半透明标志"/>
          <propoption name ="sendWheel2Hover" extendType="undef" options="是:1|否:0" description="将滚轮消息发送到hover窗口"/>
          <propoption name ="appWnd" extendType="undef" options="是:1|否:0"  description="APP窗口，是否在任务栏上显示按钮"/>
          <propoption name ="toolWindow" extendType="undef" options="是:1|否:0" description="设置WS_ES_TOOLWINDOW属性"/>
          <proptext name ="smallIcon" extendType="undef" description="小图标资源, 格式[资源ID:尺寸]" />
          <proptext name ="bigIcon" extendType="undef" description="大图标资源, 格式[资源ID:尺寸]" />
          <proptext name="alpha" extendType="undef" description="宿主透明度,0-255"/>
          <propoption name ="allowSpy" extendType="undef" options="是:1|否:0" description="在编译的SOUI关闭DISABLE_SWNDSPY宏的情况下，通过该属性来控制当前窗口是不允许Spy"/>
          <propoption name ="appMainWnd" extendType="undef" options="是:1|否:0" description="窗口显示时是否在任务栏显示按钮"/>
          <propoption name ="wndType" extendType="undef" options="undefine|appMain|normal" description="主窗口标志,有该appMain标志的窗口关闭时自动发送WM_QUIT"/>
        </propgroup>
      </groups>
    </SOUI>

    <base visible="0">
      <groups>
        <propgroup title="属性:SObject"  description="属性" expanded="false">
          <proptext title="name" name="name" extendType="undef" value="" description="Object Name"/>
          <proptext title="ID" name="id" extendType="undef" value="" description="Object ID"/>
        </propgroup>
      </groups>
    </base>

    <layout_soui visible="0">
      <groups>
        <propgroup title="锚点布局属性"  description="父窗口为锚点布局时有效的属性"  expanded="false">
          <proptext title="位置" name ="pos" extendType="undef" description="“|”代表参考父窗口的中心
				      &#x000A;“%”代表在父窗口的百分比，可以是小数，负数
				      &#x000A;“[”相对于前一兄弟窗口的右值(right,bottom)
				      &#x000A;“]”相对于后一兄弟窗口的左值(left,top)
				      &#x000A;“{”相对于前一兄弟窗口的左值(left,top)
				      &#x000A;“}”相对于后一兄弟窗口的右值(right,bottom)			
	            &#x000A;“@”标志用来指定窗口的大小，只能出现在pos属性的第3，4个值中，用来标识窗口的宽度。当后面的值为负时，代表自动计算窗口的宽度或者高度" />
          <propsize title="偏移" name="offset" extendType="undef" childrenTitles="水平偏移系数|垂直偏移系数" description="根据pos确定位置后再做偏移，单位为控件大小" />
        </propgroup>
      </groups>
    </layout_soui>
    <layout_grid visible="0">
      <groups>
        <propgroup title="网格布局属性"  description="父窗口为网格布局时有效的属性"  expanded="false">
          <proptext name ="xInterval" extendType="dim" autoComplete="1" description="Grid布局子控件的水平间距"/>
          <proptext name ="yInterval" extendType="dim" autoComplete="1" description="Grid布局子控件的垂直间距"/>
          <propoption name ="xGravity" extendType="undef" options="左:left|上:top|中:center|右:right|下:bottom|填充:fill" description="Grid布局子控件的水平对齐方式"/>
          <propoption name ="yGravity" extendType="undef" options="左:left|上:top|中:center|右:right|下:bottom|填充:fill" description="Grid布局子控件的垂直对齐方式"/>
          <proptext name ="columnCount" extendType="undef" description="Grid布局列数"/>
          <proptext name ="rowCount" extendType="undef" description="Grid布局行数"/>

          <proptext name ="rowSpan" extendType="undef" description="Grid布局,子控件行合并" />
          <proptext name ="columnSpan" extendType="undef" description="Grid布局,子控件列合并" />
          <propoption name ="layout_xGravity" extendType="undef" options="左:left|上:top|中:center|右:right|下:bottom|填充:fill" description="Grid布局,相对于父控件的水平对齐方式" />
          <propoption name ="layout_yGravity" extendType="undef" options="左:left|上:top|中:center|右:right|下:bottom|填充:fill" description="Grid布局,相对于父控件的垂直对齐方式" />
          <proptext name ="columnWeight" extendType="undef" description="Grid布局,子控件占用几列" />
          <proptext name ="rowWeight" extendType="undef" description="Grid布局,子控件占用几行" />
        </propgroup>
      </groups>
    </layout_grid>
    <layout_linear visible="0">
      <groups>
        <propgroup title="线性布局属性"  description="父窗口为线性布局时有效的属性"  expanded="false">
          <propoption name ="orientation" extendType="undef" options="horizontal|vertical" description="排列方式,默认水平布局" />
          <proptext name ="weight" extendType="undef" description="父窗口在根据size属性布局完所有子窗口后，在当前布局方向上剩下的空间的分配权重" />
        </propgroup>
      </groups>
    </layout_linear>

    <window icon="0" parent="base|layout_linear|layout_soui|layout_grid">
      <window/>
      <groups>
        <propgroup title="属性:SWindow"  description="属性">
          <proptext title="text" name="text" extendType="string" autoComplete="1" value="" description="text value"/>
          <propoption title="水平对齐" name="align"  value="" options="left|center|right" description="水平对齐方式"/>
          <propoption title="垂直对齐" name="valign" value="" options="top|middle|bottom" description="垂直对齐方式"/>
          <propcolor title="背景色" name="colorBkgnd" extendType="color" autoComplete="1" value="" description="背景颜色，不指定skin时有效"/>
          <propcolor title="边框颜色" name="colorBorder" extendType="color" autoComplete="1" value="" description="边框颜色，需要和margin一起使用"/>
          <proptext title="文本字体" name="font" extendType="font" description="文本字体"/>
          <proptext title="Hover字体" name="fontHover" extendType="font" description="Hover状态文本字体"/>
          <proptext title="Push字体" name="fontPush" extendType="font" description="Push状态文本字体"/>
          <proptext title="Disable字体" name="fontDisable" extendType="font" description="Disable状态文本字体"/>
          <propcolor title="文本颜色" name="colorText" extendType="color" autoComplete="1" value="" description="文本颜色"/>
          <propcolor title="Hover颜色" name="colorTextHover" extendType="color" autoComplete="1" value="" description="Hover状态文本颜色"/>
          <propcolor title="Push颜色" name="colorTextPush" extendType="color" autoComplete="1" value="" description="Push状态文本颜色"/>
          <propcolor title="Disable颜色" name="colorTextDisable" extendType="color" autoComplete="1" value="" description="Disable状态文本颜色"/>
          <proprect title="边框大小" name="margin" extendType="dim" autoComplete="1" childrenTitles="左|上|右|下"  description="框架大小，和colorBorder或者ncSkin配合使用"/>
          <proprect title="内边距" name="padding" extendType="dim" autoComplete="1" childrenTitles="左|上|右|下"  description="控件内边距，和inset属性相同"/>
          <propoption title="消息穿透" name="msgTransparent" extendType="undef" value="" options="是:1|否:0" description="鼠标消息穿透，为TRUE是，控件不拦截鼠标消息"/>
          <propoption title="省略号显示文本" name ="dotted" extendType="undef" options="是:1|否:0" description="是否支持省略号显示文本" />
          <proptext title="透明度" name ="alpha" extendType="dim" autoComplete="1" description="数值在0-255之间"/>
          <propoption title="跟踪鼠标" name ="trackMouseEvent" extendType="undef" options="是:1|否:0" description="窗口不在最顶层时可以收到mouseHover,mouseLeave消息" />
          <propoption title="背景融合" name ="blendBackground" extendType="undef" options="是:1|否:0" description="渲染窗口内容和背景混合标志，默认为TRUE，为FALSE代表窗口不需要和背景做融合，如视频显示窗口" />

          <proptext title="皮肤" name="skin" extendType="skin" autoComplete="1"/>
          <proptext title="边框皮肤" name ="ncskin" extendType="skin" autoComplete="1"/>
          <propoption title="样式" name ="class" extendType="string" dropDown="false"/>
          <propoption title="可用" name ="enable" extendType="undef" options="是:1|否:0"/>
          <propoption  title="可见" name ="visible" extendType="undef" options="是:1|否:0" description="和show属性一样"/>
          <propoption title="画布缓存" name ="cache" extendType="undef" options="是:1|否:0" description="将窗口绘制图像保存到缓存上，以加速渲染"/>
          <propoption title="隐藏占位" name="display" extendType="undef" options="是:1|否:0" description="控件隐藏时是否占位0不占，1占，默认为1"/>
          <proptext title="提示" name ="tip" extendType="string" />
          <proptext title="用户数据" name ="data" extendType="string" />
          <proptext title="最大宽度" name ="maxWidth" extendType="dim" autoComplete="1" description="自动计算大小时，窗口的最大宽度"/>
          <propoption title="裁剪客户区" name ="clipClient" extendType="undef" options="是:1|否:0" description="限制子窗口渲染不超出当前窗口，默认False,可以减少一次裁剪操作"/>
          <propoption  title="焦点" name ="focusable" extendType="undef" options="是:1|否:0" description="窗口是否可获得焦点标志"/>
          <propoption  title="焦点虚框" name ="drawFocusRect" extendType="undef" options="是:1|否:0" description="drawFocusRect:绘制默认的焦点虚框"/>

          <propsize title="尺寸" name ="size" extendType="dim" autoComplete="1" childrenTitles="宽度|高度" description="-1   窗口自适应内容大小
			&#x000A;-2   使用父窗口的宽/高" />

          <propoption title="光标" name ="cursor" extendType="undef" options="arrow|ibeam|wait|cross|uparrow|size|sizenwse|sizenesw|sizewe|sizens|sizeall|no|hand|help" description="鼠标形状"/>
          <propoption title="float" name ="float" extendType="undef" options="是:1|否:0" description="窗口位置固定不动的标志"/>
          <propoption  title="layout" name ="layout" extendType="undef" options="锚点布局:soui|Grid布局:gridLayout|线性水平:hbox|线性垂直:vbox" description="布局形式"/>
          <propoption title="gravity" name ="gravity" extendType="undef" options="左:left|上:top|中:center|右:right|下:bottom" description="子控件的对齐方式"/>
          <propoption title="layout_gravity" name ="layout_gravity" extendType="undef" options="左:left|上:top|中:center|右:right|下:bottom" description="相对于父控件的对齐方式"/>


          <proptext  name="extend" extendType="dim" autoComplete="1" title ="外边距(extend)" description='extend = "0,0,0,0"
		&#x000A;四个点分别表示左上右下的间距，与margin类似;' />

        </propgroup>
      </groups>
    </window>

    <text icon="21" parent="window">
      <text text="text"/>
      <groups>
        <propgroup name="属性:text">
          <proptext name ="interHeight" extendType="dim" autoComplete="1" description="行间距"/>
          <propoption name ="wordBreak" extendType="undef" options="0|1" description="是否以字符为换行单位"/>
        </propgroup>
      </groups>
    </text>

    <div visible="0" parent="window">
      <groups>
        <propgroup title="属性:SPanel"  description="SPanel属性">
          <proptext name="sbSkin" extendType="skin" autoComplete="1" description="滚动条皮肤"/>
          <proptext name="sbArrowSize" extendType="dim" autoComplete="1" description="滚动条箭头大小"/>
          <proptext name="sbWid" extendType="dim" autoComplete="1" description="滚动条宽度"/>
          <propoption name="sbEnable" extendType="undef" options="启用:true|禁用:false" description="滚动条启用/禁用"/>
          <proptext name="updateInterval" extendType="undef" description="滚动时刷新时间间隔"/>
          <proptext name="scrollSpeed" extendType="undef" description="滚动速度"/>

          <proptext name="sbLeft" extendType="dim" autoComplete="1" description="滚动左留空"/>
          <proptext name="sbRight" extendType="dim" autoComplete="1" description="滚动右留空"/>
          <proptext name="sbTop" extendType="dim" autoComplete="1" description="滚动上留空"/>
          <proptext name="sbBottom" extendType="dim" autoComplete="1" description="滚动下留空"/>

          <proptext name="sbFadeFrames" extendType="dim" autoComplete="1" description="滚动条动画帧数"/>
          <proptext name="sbTrumbTrackMinAlpha" extendType="dim" autoComplete="1" description="滚动条滑块最小alpha值"/>
        </propgroup>
      </groups>
    </div>

    <scrollview icon="1" parent="div">
      <scrollview pos="0,0" size="120,120" viewsize="-1,-1">
      </scrollview>
      <groups>
        <propgroup title="属性:scrollview"  description="scrollview属性">
          <proptext name="origin-x" extendType="dim" autoComplete="1" description="原点x位置"/>
          <proptext name="origin-y" extendType="dim" autoComplete="1" description="原点y位置"/>
          <propsize name="viewSize" extendType="dim" autoComplete="1" description="视图大小"/>
        </propgroup>
      </groups>
    </scrollview>

    <animateimg text="图片动画" icon="2" parent="window">
      <animateimg pos="0,0" size="96,96" autostart="1"  speed = "100"/>
      <groups>
        <propgroup title="属性:animateimg"  description="animateimg属性">
          <proptext name="speed" extendType="undef" description="切换帧时间"/>
          <propoption name="autoStart" extendType="undef" options="启用:true|禁用:false" description="显示时自动播放动画"/>
          <proptext name="repeat" extendType="undef" description="重复播放次数,默认无限循环"/>
        </propgroup>
      </groups>
    </animateimg>

    <button icon="3" parent="window">
      <button pos="0,0" size="96,32" colorText="#000000" skin="_skin.sys.btn.normal" >按钮</button>
      <groups>
        <propgroup title="属性:button"  description="button属性">
          <proptext name ="accel" extendType="undef" description="快捷键"/>
          <propoption name ="animate" extendType="undef" options="无:0|有:1" description="浮动状态是否显示动画"/>
          <proptext name="animateStep" extendType="undef" description="动画切换帧数,帧数越大，动画过度越平滑，耗时越长"/>
          <propoption name="disableAccelIfInvisible" extendType="undef" options="启用:true|禁用:false" description="隐藏时不接收快捷键"/>
        </propgroup>
      </groups>
    </button>

    <calendar icon="4" parent="window">
      <calendar size="96,96" daySkin="_skin.sys.btn.normal" footerHeight="30"/>
      <groups>
        <propgroup title="属性:calendar"  description="calendar属性">
          <proptext name ="yearHeight" extendType="dim" autoComplete="1" description="年月高度"/>
          <proptext name ="weekHeight" extendType="dim" autoComplete="1" description="星期高度"/>
          <proptext name ="todayHeight" extendType="dim" autoComplete="1" description="今天高度"/>
          <propcolor name ="colorSelText" extendType="color" autoComplete="1" description="选中日期文字颜色"/>
          <propcolor name ="colorOtherText" extendType="color" autoComplete="1" description="其它日期颜色"/>
          <propcolor name ="colorSelBg" extendType="color" autoComplete="1" description="选中背景颜色"/>
          <propcolor name ="colorHoverText" extendType="color" autoComplete="1" description="浮动日期文字颜色"/>
          <proptext name="prevSkin" extendType="skin" autoComplete="1" description="上翻页按钮皮肤"/>
          <proptext name="nextSkin" extendType="skin" autoComplete="1" description="下翻页按钮皮肤"/>
          <proptext name="daySkin" extendType="skin" autoComplete="1" description="今天按钮皮肤"/>
          <proptext name="weekSkin" extendType="skin" autoComplete="1" description="星期按钮皮肤"/>

          <proptext name="textSunday" extendType="string" description="星期天文本，支持多语言"/>
          <proptext name="textMonday" extendType="string" description="星期一文本，支持多语言"/>
          <proptext name="textTuesday" extendType="string" description="星期二文本，支持多语言"/>
          <proptext name="textWednesday" extendType="string" description="星期三文本，支持多语言"/>
          <proptext name="textThursday" extendType="string" description="星期四文本，支持多语言"/>
          <proptext name="textFriday" extendType="string" description="星期五文本，支持多语言"/>
          <proptext name="textSaturday" extendType="string" description="星期六文本，支持多语言"/>
        </propgroup>
      </groups>

    </calendar>

    <caption  icon="0" parent="window">
      <caption size="-2, 32" layout="hbox" gravity="center">
        <text>标题</text>
        <window size="0,0" weight="1" visible="0"/>
        <imgbtn name="btn_min" skin="_skin.sys.btn.minimize" animate="1" />
        <window>
          <imgbtn name="btn_max" skin="_skin.sys.btn.maximize"  pos="0,0" animate="1" />
          <imgbtn name="btn_restore" skin="_skin.sys.btn.restore"  pos="0,0" show="0" animate="1" />
        </window>
        <imgbtn name="btn_close" skin="_skin.sys.btn.close" tip="close" animate="1"/>
      </caption>
    </caption>


    <check icon="5" parent="window">
      <check checked="0">复选框</check>
      <groups>
        <propgroup title="属性:check"  description="check属性">
          <proptext name ="focusSkin" extendType="skin" autoComplete="1" description="焦点状态显示的皮肤"/>
          <propoption name ="checked" extendType="undef" options="是:1|否:0" description="初始化时的选中状态"/>
        </propgroup>
      </groups>
    </check>

    <toggle icon="23" parent="check">
      <toggle pos="0,0" offset="0,-0.5" name="tgl_switch" skin="_skin.sys.tree.toggle" />
    </toggle>

    <radio icon="6" parent="window">
      <radio pos="0,0" size="96,32">radio 1</radio>
      <propgroup title="属性:radio"  description="radio属性">
        <proptext name ="focusSkin" extendType="skin" autoComplete="1" description="焦点状态显示的皮肤"/>
        <propoption name ="checked" extendType="undef" options="是:1|否:0" description="初始化时的选中状态"/>
      </propgroup>
    </radio>

    <combobase visible="0" parent="window">
      <groups>
        <propgroup name="属性: combobase" description="combobase属性">
          <propoption name ="dropDown" extendType="undef" options="下拉:1|编辑:0" description="下拉样式或编辑样式"/>
          <proptext name ="dropHeight" extendType="dim" autoComplete="1" description="下拉列表高度"/>
          <proptext name ="curSel" extendType="dim" autoComplete="1" description="选中项"/>
          <proptext name ="btnSkin" extendType="skin" autoComplete="1" description="下拉按钮皮肤"/>
          <proptext name ="animateTime" extendType="dim" autoComplete="1" description="下拉框弹出动画时长"/>
          <propoption name ="autoFitDropBtn" extendType="undef" options="是:1|否:0" description="自适应下拉按钮皮肤" />
        </propgroup>
      </groups>
    </combobase>
    <combobox icon="7" parent="combobase">
      <combobox size="120, 24" dropDown="1" dropHeight="150" dotted="0" animateTime="200" curSel="0" drawFocusRect="0" colorText="#000000ff" colorBkgnd="#FFFFFF">
        <liststyle iconSkin="" itemSkin="" itemHeight="24" colorText="#000000" colorSelText="#FFFFFF" colorItemBkgnd="#FFFFFF" colorItemSelBkgnd="rgb(51,153,255)" />
        <editstyle inset="5,0,5,0" colorText="#000000" align="left" colorBkgnd="#FFFFFF" />
        <items>
          <item text="" />
          <item text="5" icon="2" data="100" />
          <item text="6" icon="3" data="103" />
          <item text="7" icon="4" data="100" />
          <item text="8" icon="5" data="105" />
          <item text="9" icon="0" data="100" />
        </items>
      </combobox>
    </combobox>


    <comboview icon="7" parent="combobase">
      <comboview size="120,24"  dropDown="0" dropHeight="200" animateTime="0"  ncSkin="_skin.sys.border" margin-x="2" margin-y="2" curSel="0" id_text="3">
