<root>
  <toolbar>
    <icons class_name="imglist" name="skin_icon" src="file:img/skin_icons.png" states="10"/>
  </toolbar>
  <skins>
    <base visible="0">
      <groups>
        <propgroup title="属性:  base"  description="属性">
          <proptext title="皮肤名(name)" name="name" value=""/>
          <proptext title="比例(scale)" name="scale" value=""/>
          <proptext title="alpha" name="alpha" value=""/>
          <propoption title="enableColorize" name="enableColorize" options="是:1|否:0"/>
        </propgroup>
      </groups>
    </base>
    <imglist icon="0" parent="base">
      <groups>
        <propgroup title="属性:  imglist"  description="属性">
          <propoption title="autoFit" name="autoFit" value="" options="是:1|否:0"/>
          <propoption title="平铺(tile)" name="tile" value="" options="拉伸:0|平铺:1"/>
          <propoption title="垂直排列(vertical)" name="vertical" value="" options="垂直:1|水平:0"/>
          <proptext title="子图数量(states)" name="states" value=""/>
          <proptext title="数据源(src)" name="src" value="" extendType="imageSrc" autoComplete="1"/>
          <propoption title="filterLevel" name="filterLevel" value="" options="none|low|medium|high"/>
        </propgroup>
      </groups>
    </imglist>

    <imgcenter icon="1" parent="imglist"/>

    <imgframe icon="2"  parent="imglist">
      <groups>
        <propgroup title="属性:  imgframe"  description="属性">
          <proptext title="左边距(left)" name="left" value=""/>
          <proptext title="上边距(top)" name="top" value=""/>
          <proptext title="右边距(right)" name="right" value=""/>
          <proptext title="下边距(bottom)" name="bottom" value=""/>
          <proptext title="九宫格4周(margin)" name="margin" value=""/>
          <proptext title="左右边距(margin-x)" name="margin-x" value=""/>
          <proptext title="上下边距(margin-y)" name="margin-y" value=""/>
        </propgroup>
      </groups>
    </imgframe>

    <imgframe2 icon="3" parent="imgframe"/>


    <Button icon="3" parent="base">
      <groups>
        <propgroup title="属性:  Button"  description="属性">
          <propcolor title="colorBorder" name="colorBorder" value="" extendType="color" autoComplete="1"/>
          <propcolor title="colorBorderHover" name="colorBorderHover" value="" extendType="color" autoComplete="1" />
          <propcolor title="colorBorderPush" name="colorBorderPush" value="" extendType="color" autoComplete="1" />
          <propcolor title="colorBorderDisable" name="colorBorderDisable" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="正常状态渐变起始颜色" name="colorUp" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="正常状态渐变终止颜色" name="colorDown" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="浮动状态渐变起始颜色" name="colorUpHover" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="浮动状态渐变终止颜色" name="colorDownHover" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="下压状态渐变起始颜色" name="colorUpPush" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="下压状态渐变终止颜色" name="colorDownPush" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="禁用状态渐变起始颜色" name="colorUpDisable" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="禁用状态渐变终止颜色" name="colorDownDisable" value=""  extendType="color" autoComplete="1"/>
          <proptext title="圆角大小" name="cornerRadius" value=""/>
          <proptext title="圆角 百分比" name="cornerPercent" value="0.6"/>
        </propgroup>
      </groups>
    </Button>

    <gradation icon="4" parent="base">
      <groups>
        <propgroup title="属性:  gradation"  description="属性">
          <proptext title="渐变起始颜色(colorFrom)" name="colorFrom" value=""  extendType="color" autoComplete="1"/>
          <proptext title="渐变终止颜色(colorTo)" name="colorTo" value="" extendType="color" autoComplete="1"/>
          <propoption title="渐变方向(vertical)" name="vertical" value="" options="水平:0|垂直:1"/>
        </propgroup>
      </groups>
    </gradation>

    <scrollbar icon="5"  parent="imglist">
      <groups>
        <propgroup title="属性:  scrollbar"  description="属性">
          <proptext title="边缘不拉伸大小(margin)" name="margin" value=""/>
          <propoption title="滑块帮手(hasGripper)" name="hasGripper" value="" options="是:1|否:0"/>
          <propoption title="是否有禁用态(hasInactive)" name="hasInactive" value="" options="是:1|否:0"/>
        </propgroup>
      </groups>
    </scrollbar>

    <Ani visible="0" parent="base">
      <groups>
        <propgroup title="属性:  Ani"  description="属性">
          <proprect title="margin" name="margin" value="" childrenTitles="左|上|右|下"  description="边缘不拉伸大小，使用默认单位"/>
          <proprect title="margin2" name="margin2" digit="false" value=""  childrenTitles="左|上|右|下" description="边缘不拉伸大小，可以指定单位如dp,px"/>
          <propoption title="平铺(Tile)" name="tile" value="" options="是:1|否:0"/>
          <propoption title="filterLevel" name="filterLevel" value="" options="none|low|medium|high"/>
        </propgroup>
      </groups>
    </Ani>
    <gif icon="6" parent="Ani">
      <groups>
        <propgroup title="属性:  gif"  description="属性">
          <proptext title="数据源(src)" name="src" value="" extendType="imageSrc" autoComplete="1"/>
        </propgroup>
      </groups>
    </gif>

    <ColorRect icon="7" parent="base">
      <groups>
        <propgroup title="属性:  ColorRect"  description="属性">
          <propcolor title="normal" name="normal" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="hover" name="hover" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="pushdown" name="pushdown" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="disable" name="disable" value=""  extendType="color" autoComplete="1"/>
          <proptext title="圆角" name="cornerRadius" value=""/>
          <proptext title="圆角 百分比" name="cornerPercent" value="0.6"/>
        </propgroup>
      </groups>
    </ColorRect>

    <Shape icon="8"  parent="base">
      <groups>
        <propgroup title="属性:  Shape"  description="属性">
          <propoption title="type" name="type" options="linear|radial|sweep"/>
          <proptext title="渐变角度(angle)" name="angle" value=""/>
          <proptext title="渐变中心(centerX)" name="centerX" value="0.5"/>
          <proptext title="渐变中心(centerY)" name="centerY" value="0.5"/>
          <propcolor title="渐变开始点(startColor)" name="startColor" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="渐变中间点(centerColor)" name="centerColor" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="渐变结束点(endColor)" name="endColor" value=""  extendType="color" autoComplete="1"/>
          <propcolor title="渐变半径(radius)" name="radius" value="" />
        </propgroup>
      </groups>
    </Shape>

    <Group icon="9"   parent="base">
      <groups>
        <propgroup title="属性:  Group"  description="属性">
          <proptext title="正常皮肤" name="normal" value=""/>
          <proptext title="浮动皮肤" name="hover" value="0.5"/>
          <proptext title="按下的皮肤" name="pushDown" value="0.5"/>
          <proptext title="禁止的皮肤" name="disable" value="0.5"/>
        </propgroup>
      </groups>
    </Group>
  </skins>
</root>
